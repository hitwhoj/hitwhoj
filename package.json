    "toml": "^3.0.0",
    "ws": "^8.8.1",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/minio": "^7.0.12",
    "@types/morgan": "^1.9.3",
