    "@types/ws": "^8.5.3",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.0.0",
    "daisyui": "^2.24.0",
    "esbuild": "^0.14.21",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.12.0",
    "eslint-formatter-pretty": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prisma": "^4.11.0",
    "shx": "^0.3.4",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.1.2",
    "husky": "^8.0.3",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.7",
    "prettier-plugin-prisma": "^4.10.0",
